    "@types/crypto-js": "^4.2.2",
    "@types/jest": "^29.5.13",
    "@types/node": "^24.10.0",
    "@types/pako": "^2.0.3",
    "@types/react": "^18.3.0",
    "@types/react-test-renderer": "^18.3.0",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "jest-environment-node": "^30.2.0",
    "prettier": "2.8.8",
    "react-native-dotenv": "^3.4.11",
    "react-test-renderer": "18.3.1",
    "typescript": "^5.8.3"
